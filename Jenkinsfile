pipeline {    agent any    stages {        stage('Debug Workspace') {            steps {                sh 'pwd'                sh 'ls -la'                sh 'echo "Workspace path: $WORKSPACE"'                sh 'ls -la $WORKSPACE'            }        }                stage('Checkout') {            steps {                git branch: 'main', url: 'https://github.com/DerazorDede/aws-elastic-beanstalk-express-js-sample.git'            }        }                stage('Debug After Checkout') {            steps {                sh 'pwd'                sh 'ls -la'            }        }    }}