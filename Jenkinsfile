pipeline {    agent any    stages {        stage('Checkout') {            steps {                git branch: 'main', url: 'https://github.com/DerazorDede/aws-elastic-beanstalk-express-js-sample.git'            }        }                stage('Install Dependencies') {            steps {                sh 'pwd && ls -la'                sh 'npm install --save'            }        }                stage('Unit Test') {            steps {                sh 'npm test'            }        }                stage('Security Scan') {            steps {                sh 'npm audit --audit-level high || echo "Vulnerabilities found but continuing for demo"'            }        }                stage('Build Docker Image') {            steps {                sh 'docker build -t my-node-app:${BUILD_ID} .'            }        }    }        post {        always {            echo "Pipeline completed!"        }    }}