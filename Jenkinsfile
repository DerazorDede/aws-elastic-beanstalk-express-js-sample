pipeline {    agent any    stages {        stage('Checkout') {            steps {                git branch: 'main', url: 'https://github.com/DerazorDede/aws-elastic-beanstalk-express-js-sample.git'            }        }                stage('Install Dependencies') {            steps {                sh 'docker run --rm -v /var/jenkins_home/workspace/23017274_Project2_pipeline:/app -w /app node:16 npm install --save'            }        }                stage('Unit Test') {            steps {                sh 'docker run --rm -v /var/jenkins_home/workspace/23017274_Project2_pipeline:/app -w /app node:16 npm test'            }        }                stage('Security Scan') {            steps {                sh 'docker run --rm -v /var/jenkins_home/workspace/23017274_Project2_pipeline:/app -w /app node:16 npm audit --audit-level high || echo "Vulnerabilities found but continuing for demo"'            }        }                stage('Build Docker Image') {            steps {                sh 'docker build -t my-node-app:${BUILD_ID} .'            }        }    }        post {        always {            echo "Pipeline completed!"        }        success {            echo "SUCCESS: All stages completed successfully!"        }    }}